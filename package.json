  "repository": {
    "type": "git",
    "url": "git+https://github.com/romelperez/react-examples.git"
  },
  "main": "server/index.js",
  "scripts": {
    "start": "NODE_PATH=./src/js/ ./node_modules/.bin/babel-node server",
    "start:tunnel": "ENABLE_TUNNEL=true npm run start",
    "start:production": "NODE_ENV=production npm run start",
    "build": "npm run build-css && npm run build-js",
    "build:production": "NODE_ENV=production npm run build-css && NODE_ENV=production npm run build-js",
    "build-css": "./node_modules/.bin/gulp sass",
    "build-css:watch": "./node_modules/.bin/gulp",
    "build-js": "./node_modules/.bin/webpack",
    "build-js:watch": "./node_modules/.bin/webpack -w",
    "test": "npm run test-src && npm run test-server && npm run test-integration",
    "test-src": "NODE_PATH=./src/js/ NODE_ENV=test ./node_modules/.bin/karma start karma.conf.js",
    "test-server": "NODE_PATH=./src/js/ NODE_ENV=test ./node_modules/.bin/mocha './test/server/setup.js' './server/**/*-test.js'",
    "test-integration": "NODE_PATH=./src/js/ NODE_ENV=test ./node_modules/.bin/mocha './test/integration/setup.js' './test/integration/**/*-test.js'",
    "docs": "npm run docs-styleguide && npm run docs-js && npm run docs-api",
    "docs-styleguide": "NODE_PATH=./src/js/ ./node_modules/.bin/styleguidist build",
    "docs-js": "./node_modules/.bin/gulp jsdoc",
    "docs-api": "./node_modules/.bin/apidoc -i server/api/ -o docs/api/"
  },
  "dependencies": {
    "apidoc": "^0.16.1",
