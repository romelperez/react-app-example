    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.11.6",
    "body-parser": "^1.15.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "chai-http": "^3.0.0",
    "cssnano": "^3.7.3",
    "enzyme": "^2.4.1",
    "express": "^4.14.0",
    "foundation-sites": "^6.2.3",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.1",
    "gulp-jsdoc3": "^0.3.0",
    "gulp-livereload": "^3.8.1",
    "gulp-postcss": "^6.1.1",
