  "main": "bin/server.js",
  "scripts": {
    "start": "node bin/server.js",
    "start:tunnel": "npm run build && ENABLE_TUNNEL=true npm run start",
    "start:production": "npm run build:production && NODE_ENV=production PORT=80 npm run start",
    "build": "npm run build-sass && npm run build-js",
    "build:production": "NODE_ENV=production npm run build-sass && NODE_ENV=production npm run build-js",
    "build-sass": "./node_modules/gulp/bin/gulp.js sass",
    "build-sass:watch": "./node_modules/gulp/bin/gulp.js",
    "build-js": "./node_modules/webpack/bin/webpack.js",
    "build-js:watch": "./node_modules/webpack/bin/webpack.js -w",
    "test": "npm run test-universal && npm run test-api",
    "test-universal": "NODE_ENV=test ./node_modules/karma/bin/karma start karma.conf.js",
    "test-api": "NODE_ENV=test ./node_modules/mocha/bin/mocha './test/api/setup.js' './test/api/**/*-test.js'",
    "docs": "npm run docs-styleguide && npm run docs-js && npm run docs-api",
    "docs-styleguide": "./node_modules/react-styleguidist/bin/styleguidist build",
    "docs-js": "./node_modules/.bin/jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md",
    "docs-api": "./node_modules/apidoc/bin/apidoc -i api/ -o docs/api/"
  },
